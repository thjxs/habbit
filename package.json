{
  "dependencies": {},
  "scripts": {
    "format": "prettier --write --single-quote *.js test/*.js",
    "hint": "jshint",
    "test": "mocha"
  },
  "devDependencies": {
    "husky": "^4.3",
    "jshint": "^2.12",
    "lint-staged": "^10.4",
    "mocha": "^8.1",
    "prettier": "^2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jshintConfig": {
    "esversion": 9
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "jshint"
    ]
  },
  "license": "MIT"
}
